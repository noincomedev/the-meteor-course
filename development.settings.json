{
  "public": {
    "name": "The Meteor Course",
    "router": {
      "index": {
        "NAME": "index",
        "PATH": "/",
        "TITLE": "HOME",
        "CONTENT": "index"
      },
      "public_routes": {
        "signin": {
          "NAME": "signin",
          "PATH": "/signin",
          "TITLE": "Login",
          "CONTENT": "signin"
        },
        "not_found": {
          "NAME": "not_found",
          "PATH": "/not_found",
          "TITLE": "Not Found",
          "CONTENT": "not_found"
        }
      },
      "private_routes": {
        "add_todo": {
          "NAME": "add-todo",
          "PATH": "/add-todo",
          "TITLE": "Add Todo",
          "CONTENT": "add-todo"
        }
      }
    },
    "constants": {
      "pages": {
        "index": {
          "HEADLINE": "TODO:LIST"
        }
      },
      "lists": {
        "todos": {
          "item": {
            "ACTION_BTN_LBL": "DONE"
          }
        }
      }
    },
    "forms": {
      "add_todo": {
        "input": {
          "PLACEHOLDER": "TODO:"
        },
        "submit": {
          "LABEL": "ADD"
        }
      },
      "accounts": {
        "signin": {
          "HEADLINE": "Account Login",
          "email": {
            "PLACEHOLDER": "Email"
          },
          "password": {
            "PLACEHOLDER": "Password"
          },
          "submit": {
            "LABEL": "GO"
          }
        },
        "signup": {
          "HEADLINE": "Create Account",
          "email": {
            "PLACEHOLDER": "Email"
          },
          "password": {
            "PLACEHOLDER": "Password"
          },
          "confirmation": {
            "PLACEHOLDER": "Repeat Password"
          },
          "username": {
            "PLACEHOLDER": "Username"
          },
          "submit": {
            "LABEL": "SUBMIT"
          }
        }
      }
    },
    "nav": {
      "logo": {
        "LABEL": "TODOFY"
      },
      "private": {
        "LINKS": [{ "key": "link1", "to": "/add-todo", "label": "ADD TODO" }]
      },
      "public": {
        "LINKS": [{ "key": "link1", "to": "/signin", "label": "SIGN IN" }]
      }
    }
  }
}
